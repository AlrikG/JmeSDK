<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<HBox alignment="CENTER_LEFT" spacing="5.0" styleClass="openProject" stylesheets="@../../JavaFx/Theme/base.css, @../../JavaFx/Theme/welcome.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jayfella.sdk.controller.list.ProjectListItem">
   <children>
      <VBox spacing="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="projectNameLabel" text="Project Name" />
            <Label fx:id="projectPathLabel" text="Project Path" />
         </children>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </VBox>
      <Button fx:id="removeButton" mnemonicParsing="false" styleClass="removeButton">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
         <graphic>
            <FontAwesomeIconView glyphName="TIMES_CIRCLE" size="24px" />
         </graphic>
      </Button>
   </children>
</HBox>
