<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" fx:id="root" stylesheets="@../../style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox styleClass="componentTitleHBox">
         <children>
            <Label fx:id="titleLabel" styleClass="componentTitle" text="Label" />
         </children>
      </HBox>
      <HBox>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
         <children>
            <ColorPicker fx:id="colorPicker" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="componentVBox" />
      <String fx:value="component" />
   </styleClass>
</fx:root>
